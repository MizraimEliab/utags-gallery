{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/dist/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/dist/favicon.ico",
          "/dist/index.html",
          "/dist/manifest.webmanifest",
          "/dist/*.css",
          "/dist/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/dist/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ]
}
